
@{
    ViewData["Title"] = "Upload Card Image";
}

<h1>Upload a Card Image</h1>

<form asp-action="UploadTemplateImage" asp-controller="Home" method="post" enctype="multipart/form-data">
    <input type="file" name="TemplateImage" accept="image/*" required />
    <button type="submit">Upload</button>
</form>
